      "legs": [
        {
          "kind": "sold_call",
          "occurredAt": "2026-01-01T10:00:00.000Z",
          "transactionId": "t1",
          "raw": null
        },
        {
          "kind": "called_away",
          "occurredAt": "2026-01-08T10:00:00.000Z",
